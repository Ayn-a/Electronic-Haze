class Particle{
  int positionX;
  int positionY;
  int size;
  color c;
  int type;
  
  Particle(int positionX, int positionY, int size, color c, int type){
    this.positionX = positionX;
    this.positionY = positionY;
    this.size = size;
    this.c = c;
    this.type = type;
  }
}
